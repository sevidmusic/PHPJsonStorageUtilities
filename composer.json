{
    "name": "darling/php-json-storage-utilities",
    "description": "This library provides classes for storing and retrieving data that was encoded as json by a \\Darling\\PHPJsonUtilities\\classes\\encoded\\data\\Json instance.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sevi Donnelly Foreman",
            "email": "sdmwebsdm@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "darling/php-unit-test-utilities": "^1.0",
        "darling/php-text-types": "^1.0",
        "darling/php-darling-dev-tools": "^1.0",
        "darling/php-json-utilities": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Darling\\PHPJsonStorageUtilities\\": "src/",
            "Darling\\PHPJsonStorageUtilities\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*"
    }
}
